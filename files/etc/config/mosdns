config mosdns 'config'
	option enabled '1'
	option configfile '/etc/mosdns/config.yaml'
	option listen_port '5335'
	option log_level 'error'
	option logfile '/tmp/mosdns.log'
	option redirect '1'
	option custom_local_dns '1'
	list local_dns '119.29.29.29'
	list local_dns '223.5.5.5'
	list local_dns '114.114.114.114'
	list local_dns '180.76.76.76'
	option bootstrap_dns '119.29.29.29'
	list remote_dns 'tls://1.1.1.1'
	list remote_dns 'tls://8.8.8.8'
	list remote_dns 'tls://208.67.222.222'
	option enable_pipeline '1'
	option cache_size '200000'
	option cache_survival_time '259200'
	option minimal_ttl '0'
	option maximum_ttl '0'
	option adblock '1'
	option ad_source 'geosite.dat'
	option geo_auto_update '1'
	option geo_update_week_time '*'
	option geo_update_day_time '7'